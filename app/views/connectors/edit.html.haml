%h2 Edit Connector
= form_for @connector do |f|

  = f.label :connector_type
  = f.select :connector_type, ["Zuora"]

  = f.label :username
  = f.text_field :username

  = f.label :password
  = f.password_field :password

  = f.label :url, "URL"
  = f.select :url, @url_values

  .form-actions
    = f.submit "Update", class: "btn"
